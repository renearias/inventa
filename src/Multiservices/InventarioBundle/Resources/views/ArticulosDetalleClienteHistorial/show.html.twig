{% extends app.request.isXmlHttpRequest ? '::baseajax.html.twig' : '::basesmartpanel.html.twig' %}
{% block content -%}
    <h1>ArticulosDetalleClienteHistorial</h1>

    <table class="record_properties table">
        <tbody>
             <tr>
                <th>Articulo</th>
                 <td>{{ entity.codarticulodetalle }}</td>
            </tr>
            <tr>
                <th>Cliente</th>
                 <td>{{ entity.codusuario }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                 <td>{{ entity.fecha|default('0')|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ entity.tipo }}</td>
            </tr>
            <tr>
                <th>Observaci√≥n</th>
                <td>{{ entity.observacion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="#{{ path('articulosdetalleclientehistorial') }}">
            Regresar a la lista
        </a>
    </li>
    <li>
        <a href="#{{ path('articulosdetalleclientehistorial_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form_start(delete_form) }}{{ form_widget(delete_form.submit, { 'label': 'Eliminar'}) }}{{ form_end(delete_form) }}</li>
</ul>
    <script type="text/javascript" src="{{ asset('js/arxis-postforms.js')}}"></script> 
{% endblock %}
