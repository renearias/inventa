{% extends app.request.isXmlHttpRequest ? '::baseajax.html.twig' : '::basesmartpanel.html.twig' %}

{% block content -%}
    <h1>Atributos</h1>

    <table class="record_properties table">
        <tbody>
            <tr>
                <th>Atributo</th>
                <td>{{ entity.atributo }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="#{{ path('atributos') }}">
            Regresar a la lista
        </a>
    </li>
    <li>
        <a href="#{{ path('atributos_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form_start(delete_form) }}{{ form_widget(delete_form.submit, { 'label': 'Eliminar'}) }}{{ form_end(delete_form) }}</li>
</ul>
<script type="text/javascript" src="{{ asset('js/arxis-postforms.js')}}"></script>
<script type="text/javascript">
	pageSetUp();
	var pagefunction = function() {
		//console.log("cleared");
	};
</script> 
{% endblock %}

